{
  "name": "tts-medical-management-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "dev:node22": "nodemon --watch src --exec 'node --loader ts-node/esm src/server.ts'",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:node22": "node --test src/**/*.test.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postbuild": "node addJsExtension.js",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t tts-api:latest .",
    "docker:dev": "docker-compose up -d api-dev",
    "docker:prod": "docker-compose up -d api-prod",
    "docker:test": "chmod +x test-docker.sh && ./test-docker.sh",
    "performance": "node --prof dist/index.js",
    "perf-report": "node --prof-process isolate-*.log > performance-report.txt"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@react-email/components": "^0.0.32",
    "@react-email/render": "^1.0.4",
    "@react-email/tailwind": "^1.0.4",
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase-admin": "^13.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.13.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "openai": "^4.85.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.12.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
